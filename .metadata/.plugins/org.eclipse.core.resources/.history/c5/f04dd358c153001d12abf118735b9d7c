#include "main.h"
#include "input_processing.h"


void fsm_for_input_processing(void){
	HAL_Init();
	/* configure the system clock */
	SystemClock_Config();
	/* Initialize all configured peripherals */
	MX_GPIO_Init();
	MX_TIM2_Init();
}
